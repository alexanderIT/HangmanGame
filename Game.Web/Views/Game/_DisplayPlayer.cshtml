@model IEnumerable<Game.Web.Models.PlayerViewModel>
<table class="table table-bordered table-condensed ">
    <tr>
        <th class="alert alert-info">
            Index
        </th>
        <th class="alert alert-info">
            @Html.DisplayNameFor(model => model.FirstOrDefault().Name)
        </th>
        <th class="alert alert-info">
            @Html.DisplayNameFor(model => model.FirstOrDefault().Score)
        </th>
        <th class="alert alert-info">
            @Html.DisplayNameFor(model => model.FirstOrDefault().WinGames)
        </th>
        <th class="alert alert-info">
            @Html.DisplayNameFor(model => model.FirstOrDefault().LoseGames)
        </th>
        <th class="alert alert-info">
            @Html.DisplayNameFor(model => model.FirstOrDefault().MadeAssumptions)
        </th>
        <th class="alert alert-info">
            @Html.DisplayNameFor(model => model.FirstOrDefault().GuessWholeWord)
        </th>
</table>

